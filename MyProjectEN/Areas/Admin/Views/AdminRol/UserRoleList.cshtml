
@using EntityLayer.Concrete
@model List<AppUser>


@{
    ViewData["Title"] = "UserRoleList";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>User Role List</h1>
<table class="table table-bordered">
    <tr>
        <th>User </th>
        <th>Assign a role</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td><a href="/Admin/AdminRol/AssignRole/@item.Id/" class="btn btn-outline-warning">Assign a role</a></td>
        </tr>
    }

</table>
<a href="/Admin/AdminRol/Index/" class="btn btn-primary">Back to Role List</a>